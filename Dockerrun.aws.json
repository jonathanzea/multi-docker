{
	"AWSEBDockerrunVersion":2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "zetajonathan/multi-client",
			"hostname": "client",
			"essential":false
		}, 
		{	"name": "server",
			"image": "zetajonathan/multi-server",
			"hostname": "api",
			"essential": false
		},
		{	"name": "worker",
			"image": "zetajonathan/multi-worker",
			"hostname": "worker",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort":80
				}
			],
			"links": ["client","server"]
		}
	]
}

